<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>DrtWindow</AssemblyName>
    <LinkSubsystem>console</LinkSubsystem>
    <AssemblyIdentityVersionName>WCP</AssemblyIdentityVersionName>
    <OutputType>EXE</OutputType>
    <GenerateAssemblyAttribute>false</GenerateAssemblyAttribute>
    <TestCode>true</TestCode>
    <Win32Icon>app.ico</Win32Icon>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="DrtWindow.cs" />
    <Compile Include="..\..\Logger\Logger.cs" />
    <None Include="DrtWindowSecure.cmd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
