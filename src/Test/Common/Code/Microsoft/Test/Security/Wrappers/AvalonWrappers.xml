		</Type>
		<Type Name="System.Windows.Input.StagingAreaInputItem" SpecifiedMembersOnly="true">
			<Member Name="Input" />
		</Type>
		<Type Name="System.Windows.Input.ProcessInputEventArgs" SpecifiedMembersOnly="true">
			<Member Name="PeekInput" />
			<Member Name="PushInput" />
			<Member Name="PopInput" />
		</Type>

	</Assembly>

	<Assembly Name="WindowsBase">
		<Type Name="System.Windows.Threading.Dispatcher" SpecifiedMembersOnly="true">
			<Member Name="CurrentDispatcher" />
			<Member Name="BeginInvokeShutdown" />
			<Member Name="InvokeShutdown" />
			<Member Name="Hooks" />
			<Member Name="Run" />
			<Member Name="PushFrame" />
		</Type>
		<Type Name="System.Windows.Threading.DispatcherHooks" SpecifiedMembersOnly="true">
			<Member Name="DispatcherInactive" />
			<Member Name="OperationPosted" />
			<Member Name="OperationCompleted" />
			<Member Name="OperationPriorityChanged" />
			<Member Name="OperationAborted" />
		</Type>
	</Assembly>

</SecurityGen>
