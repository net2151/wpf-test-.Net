<SecurityGen>
<!--
	This is used to generate wrappers for Avalon Types that need to be in the SEE.
	The wrappers need to be hand edited to exclude API that do not demand permissions.
-->

	<Assembly Name="PresentationFramework">
		<Type Name="System.Windows.FrameworkElement" SpecifiedMembersOnly="true">
			<Member Name="Height" />
			<Member Name="Width" />
		</Type>

		<Type Name="System.Windows.Window" SpecifiedMembersOnly="true">
			<Member Name=".ctor" />
			<Member Name="Topmost" />
			<Member Name="Top" />
			<Member Name="Left" />
			<Member Name="Activate" />
			<Member Name="Show" />
			<Member Name="Close" />
			<Member Name="WindowStyle" />
			<Member Name="Content" />
			<Member Name="Background" />
			<Member Name="SizeToContent" />
		</Type>
		<Type Name="System.Windows.Interop.WindowInteropHelper" SpecifiedMembersOnly="true">
			<Member Name=".ctor" />
			<Member Name="Handle" />
		</Type>
		<Type Name="System.Windows.Navigation.NavigationWindow" SpecifiedMembersOnly="true">
			<Member Name=".ctor" />
		</Type>
		<Type Name="System.Windows.Application" SpecifiedMembersOnly="true">
			<Member Name="Current" />
			<Member Name="Shutdown" />
		</Type>
	</Assembly>

	<Assembly Name="PresentationCore">
		<Type Name="System.Windows.Interop.HwndSource" SpecifiedMembersOnly="true">
			<Member Name=".ctor" />
			<Member Name="Dispose" />
