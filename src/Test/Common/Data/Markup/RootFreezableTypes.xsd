<xs:schema 
  targetNamespace="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
  elementFormDefault="qualified" 
  attributeFormDefault="qualified" 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:clr="http://schemas.microsoft.com/clr" 
  xmlns:xml="http://www.w3.org/XML/1998/namespace">
  
  <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd" />
  <xs:include schemaLocation="3DTypes.xsd" />
  <xs:include schemaLocation="AttributeGroups.xsd" />
  <xs:include schemaLocation="BaseTypes.xsd" />
  <xs:include schemaLocation="BrushTypes.xsd" />
  <xs:include schemaLocation="ShapeTypes.xsd" />
  <xs:include schemaLocation="EffectTypes.xsd" />
  <xs:include schemaLocation="PanelTypes.xsd" />
  <xs:include schemaLocation="StyleTypes.xsd" />
  <xs:include schemaLocation="SimpleTypes.xsd" />
  <xs:include schemaLocation="TextTypes.xsd" />
  <xs:include schemaLocation="TransformTypes.xsd" />
  <xs:include schemaLocation="MiscTypes.xsd" />
  <xs:include schemaLocation="ElementGroups4.xsd" />
  
  <!-- ******************************************************* -->
  <!-- *** Brushes ******************************************* -->
  <!-- ******************************************************* -->
  <xs:element name="VisualBrush" type="dVisualBrush" />
  <xs:element name="SolidColorBrush" type="dSolidColorBrush" />
  <xs:element name="LinearGradientBrush" type="dLinearGradientBrush" />
  <xs:element name="RadialGradientBrush" type="dRadialGradientBrush" />
  <xs:element name="ImageBrush" type="dImageBrush" />

  <!-- ******************************************************* -->
  <!-- *** Effects ******************************************* -->
  <!-- ******************************************************* -->
  <xs:element name="BitmapEffectGroup" type="dBitmapEffectGroup" />
  <xs:element name="BlurBitmapEffect" type="dBlurBitmapEffect" />
  <xs:element name="DropShadowBitmapEffect" type="dDropShadowBitmapEffect" />
  <xs:element name="EmbossBitmapEffect" type="dEmbossBitmapEffect" />
  <xs:element name="OuterGlowBitmapEffect" type="dOuterGlowBitmapEffect" />
  <xs:element name="TextEffectCollection" type="dTextEffectCollection" />
  <xs:element name="TextEffect" type="dTextEffect" />

  <!-- ******************************************************* -->
  <!-- *** Transforms **************************************** -->
  <!-- ******************************************************* -->
  <xs:element name="TransformGroup" type="dTransformGroup" />
  <xs:element name="RotateTransform" type="dRotateTransform" />
  <xs:element name="ScaleTransform" type="dScaleTransform" />
  <xs:element name="SkewTransform" type="dSkewTransform" />
  
</xs:schema>